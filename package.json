{
  "name": "node-koa-ws-ts",
  "version": "1.0.0",
  "description": "restfull and websocket combine to starter",
  "tags": [
    "typescript",
    "socket.io",
    "koa"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --ignore tests/ --watch src -e ts,tsx --exec ts-node --files src/start.ts",
    "serve": "cross-env NODE_ENV=production node dist/start.js",
    "build": "rm -rf dist && tsc",
    "pro": "npx pm2 start ecosystem.config.js --env production",
    "restart": "pm2 restart ecosystem.config.js --env production",
    "stop": "npx pm2 stop ecosystem.config.js"
  },
  "author": "leo",
  "dependencies": {
    "async-redis": "^2.0.0",
    "class-validator": "^0.13.1",
    "config": "^3.3.6",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-helmet": "^6.1.0",
    "koa-jwt": "^4.0.1",
    "koa-log4": "^2.3.2",
    "koa-router": "^10.0.0",
    "koa-session2": "^2.2.10",
    "koa-swagger-decorator": "^1.7.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.1.2",
    "socket.io-redis": "^6.1.1",
    "typeorm": "^0.2.34",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "@types/async-redis": "^1.1.2",
    "@types/config": "0.0.38",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa": "^2.13.3",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/koa-helmet": "^6.0.3",
    "@types/koa-jwt": "^3.3.0",
    "@types/koa-log4": "^2.3.2",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.4",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.12.2",
    "@types/redis": "^2.8.29",
    "@types/socket.io": "^3.0.2",
    "@types/winston": "^2.4.4",
    "cross-env": "^7.0.3",
    "socket.io-client": "^4.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
